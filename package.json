{
    "name": "binance-x",
    "version": "0.1.0",
    "private": true,
    "license": "GPLv3",
    "description": "Binance exchange portfolio management app",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "watch": "vue-cli-service build --watch",
        "lint": "vue-cli-service lint"
    },
    "dependencies": {
        "binance-api-node": "0.8.20",
        "vue": "2.6.6",
        "vue-router": "3.0.2",
        "vuex": "^3.1.0",
        "vuex-persist": "^2.0.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "3.5.0",
        "@vue/cli-plugin-eslint": "3.5.0",
        "@vue/cli-service": "3.5.0",
        "babel-eslint": "10.0.1",
        "copy-webpack-plugin": "5.0.2",
        "eslint": "5.8.0",
        "eslint-plugin-vue": "5.0.0",
        "svg-sprite-loader": "4.1.3",
        "svgo": "1.2.1",
        "svgo-loader": "2.2.0",
        "vue-template-compiler": "2.6.6"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "quotes": [
                "error",
                "single"
            ],
            "indent": [
                "error",
                4
            ],
            "max-len": [
                "error",
                {
                    "code": 250
                }
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 11"
    ]
}
